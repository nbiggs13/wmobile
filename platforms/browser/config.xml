<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mycfs.wolfmobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Wolf</name>
    <description>
        Mobile application for Wolf Sizing System.
    </description>
    <author email="" href="http://mycfs.com">
        Computerway Food Systems
    </author>
    <content src="index.html" />
    <splash src="www/res/icon/cfslogo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
